<!DOCTYPE html>
<html lang="{{ shop.language }}">
  
  <head>
    {% include 'blocks/head.rain' %}
    <title>{{ page.title }} - {{ shop.name }}</title>
    <meta name="description" content="{{ shop.description }}" />
    <meta name="keywords" content="{{ shop.keywords }}" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="shortcut icon" href="{{ 'favicon.ico' | url_asset }}" type="image/x-icon" />{% if theme.font != 'Arial' %}
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family={{ theme.font }}:400,700" type="text/css" />{% endif %}
    
    
    
    <link rel="stylesheet" href="{{ 'gui-2-0.css' | url_core }}" />
    <link rel="stylesheet" href="{{ 'gui-responsive-2-0.css' | url_core }}" />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />
    <link rel="stylesheet" href="{{ 'flexslider.css' | url_asset }}" />
    <link rel="stylesheet" href="{{ 'navigatie.css' | url_asset }}" />
    <link rel="stylesheet" href="{{ 'yamm.css' | url_asset }}" />
    <link rel="stylesheet" href="{{ 'stylesheet.css' | url_asset }}" />
    <link rel="stylesheet" href="{{ 'settings.css' | url_asset }}" />
    <link rel="stylesheet" href="{{ 'custom.css' | url_asset }}" />
    
    <script src="{{ 'jquery-1-9-1.js' | url_core }}"></script>
    <script src="{{ 'jquery-migrate-1-1-1.js' | url_core }}"></script>
    <script src="{{ 'jquery-ui-1-10-1.js' | url_core }}"></script>
    <script src="{{ 'gui.js' | url_core }}"></script>    
