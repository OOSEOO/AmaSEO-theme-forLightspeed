        <img src="{{ 'usp_03_en.png' | url_asset }}" alt="{{ page.title }}" />
        {% if theme.usp_03_url_en %}</a>{% endif %}
      </div><!--col-sm-4 col-md-3 nopadding-->
      <div class="col-md-3 nopadding hidden-sm">
        {% if theme.usp_04_url_en %}<a href="{{ theme.usp_04_url_en | url }}">{% endif %}
        <img src="{{ 'usp_04_en.png' | url_asset }}" alt="{{ page.title }}" />
