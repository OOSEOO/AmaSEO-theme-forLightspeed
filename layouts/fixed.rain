      <div class="row">
         <div class="col-md-3 hidden-xs hidden-sm paddingsm">
          <div class="search">
            <form class="navbar-form" role="search" action="{{ 'search' | url }}" method="get" id="form_search2">
              <div class="input-group">
                <input type="text" class="form-control" autocomplete="off" placeholder="{{ 'Search' | t }}" name="q" id="srch-2">
                <div class="input-group-btn">
                  <button class="btn btn-search" type="submit" title="{{ 'Search' | t }}" onclick="$('#form_search2').submit();">{{ 'Search' | t }}</button>
                  <span id="iconSpan"><img src="{{ 'cross.png' | url_asset }}" id="clearField" alt="clear-search-field" /></span>
                </div>
              </div>
              <div class="autocomplete">
                <div class="arrow"></div>
                <div class="products-livesearch"></div>
