    {% endif %} 
    
    {% include 'snippets/remarketing-cart.rain' %}
    
    <header class="{% if theme.status_usp == 'Weergeven' %}{% if theme.weergave_usp_balk == 'sticky' %}usppadding{% endif %}{% endif %} {% if theme.weergave_breedte_webshop == 'Afgekaderd' %} container{% endif %}" >
      {% if theme.weergave_breedte_webshop == 'Doorlopend' %}<div class="container">{% endif %} 
      {% if theme.weergave_header == 'Optie 1' %}
      <div class="row">
        <div class="col-md-3">
          <div class="logo">
            <a href="{{ '' | url }}" title="{{ shop.title }}">
              <img src="{{ 'logo.png' | url_asset }}" alt="{{ shop.title }}" />
            </a>
          </div>
        </div><!--col-md-4-->
        
        <div class="col-md-4 col-md-offset-2 hidden-xs hidden-sm">
          <div class="search">
            <form class="navbar-form" role="search" action="{{ 'search' | url }}" method="get" id="form_search1">
              <div class="input-group">
                <input type="text" class="form-control" autocomplete="off" placeholder="{{ 'Search' | t }}" name="q" id="srch">
                <div class="input-group-btn">
                  <button class="btn btn-search" type="submit" title="{{ 'Search' | t }}" onclick="$('#form_search1').submit();">{{ 'Search' | t }}.</button>
                  <span id="iconSpan"><img src="{{ 'cross.png' | url_asset }}" id="clearField" alt="clear-search-field" /></span>
                </div><!--input-group-btn-->
              </div><!--input-group-->
              <div class="autocomplete">
                <div class="arrow"></div>
                <div class="products-livesearch"></div>
                <div class="more">
                  <a href="#">{{ 'View all results' | t }} <span>(0)</span></a>
                </div><!--more-->
                <div class="notfound">{{ 'No products found' | t }}...</div>
              </div><!--autocomplete-->
            </form>
          </div><!--search-->
        </div><!--col-md-5 hidden-xs hidden-sm-->
        <div class="col-xs-12 mobiletopbar paddingsm visible-xs visible-sm">
          <div class="row">
            <div class="col-xs-7">
              {% if shop.languages | length > 1 %}
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">{{ shop.language2.title }} <span class="caret"></span>
                </button>
                <ul class="dropdown-menu" role="menu">
                  {% for language in shop.languages %}
                  <li><a href="{{ language.url | url }}" lang="{{ language.code }}" title="{{ language.title }}">{{ language.title }}</a>
                  </li>
                  {% endfor %}
                </ul>
              </div>
              {% endif %} {% if shop.currencies | length > 1 %}
              <div class="btn-group nopaddingl">
                <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">{{ shop.currency2.symbol }} <span class="caret"></span>
                </button>
                <ul class="dropdown-menu" role="menu">
                  {% for currency in shop.currencies %}
                  <li><a href="{{ currency.url | url }}" rel="nofollow" title="{{ currency.title }}">{{ currency.symbol }}</a>
                  </li>
                  {% endfor %}
                </ul>
              </div>
              {% endif %}
            </div>
            <div class="col-xs-5">
              <ul class="header-links">
                {% if page.account %}
                <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}" class="btn-default btn btn-sm">{{ 'My account' | t }} </a></li>
                {% else %}
                <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}" class="btn-default btn btn-sm">{{ 'Login' | t }} </a></li>
                {% endif %}
              </ul>
            </div>
          </div>
        </div>
        <div class="col-md-3 paddingsm hidden-xs hidden-sm">
          <div class="header-select">
            {% if shop.languages | length > 1 %}
            <div class="btn-group">
              <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">{{ shop.language2.title }} <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu">
                {% for language in shop.languages %}
                <li><a href="{{ language.url | url }}" lang="{{ language.code }}" title="{{ language.title }}">{{ language.title }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
            {% endif %} {% if shop.currencies | length > 1 %}
            <div class="btn-group">
              <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">{{ shop.currency2.symbol }} <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu">
                {% for currency in shop.currencies %}
                <li><a href="{{ currency.url | url }}" rel="nofollow" title="{{ currency.title }}">{{ currency.symbol }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
            {% endif %}
          </div>
          <div class="pull-right paddinglg">
            <ul class="header-links">
              {% if page.account %}
              <li><a href="{{ 'account/logout' | url }}" title="{{ 'Logout' | t }}">{{ 'Logout' | t }} <span class="glyphicon glyphicon-log-out"></span></a></li>
              <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}">{{ 'My account' | t }} <span class="glyphicon glyphicon-list-alt"></span></a></li>
              {% else %}
              <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}">{{ 'Login' | t }} <span class="glyphicon glyphicon-log-in"></span></a></li>
              <li><a href="{{ 'account/register' | url }}" title="{{ 'Register' | t }}">{{ 'Register' | t }} <span class="glyphicon glyphicon-user"></span><span class="glyphicon glyphicon-plus"></span></a></li>
              {% endif %}
            </ul>
          </div><!-- header-select -->
        </div><!-- col-md-3 -->
      </div><!-- row -->
      
