    <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% if theme.loyaltylion %}{% include 'snippets/loyaltylion.rain' %}{% endif %} 
  </head>
  
  <body>
    {% if theme.weergave_breedte_webshop == 'Afgekaderd' %}<div class="boxed">{% endif %} 
    
