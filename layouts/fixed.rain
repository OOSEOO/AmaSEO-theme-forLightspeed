        <img src="{{ 'usp_03_fr.png' | url_asset }}" alt="{{ page.title }}" />
        {% if theme.usp_03_url_fr %}</a>{% endif %}
      </div><!--col-sm-4 col-md-3 nopadding-->
      <div class="col-md-3 nopadding hidden-sm">
        {% if theme.usp_04_url_fr %}<a href="{{ theme.usp_04_url_fr | url }}">{% endif %}
