                <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}" class="btn-default btn btn-sm">{{ 'My account' | t }} </a></li>
                {% else %}
                <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}" class="btn-default btn btn-sm">{{ 'Login' | t }} </a></li>
                {% endif %}
              </ul>
            </div>
          </div>
        </div>
         <div class="col-md-3 paddingsm hidden-xs hidden-sm pull-right">
          <div class="header-select">
            {% if shop.languages | length > 1 %}
            <div class="btn-group">
              <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">{{ shop.language2.title }} <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu">
                {% for language in shop.languages %}
                <li><a href="{{ language.url | url }}" lang="{{ language.code }}" title="{{ language.title }}">{{ language.title }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
            {% endif %} {% if shop.currencies | length > 1 %}
            <div class="btn-group">
              <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">{{ shop.currency2.symbol }} <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu">
                {% for currency in shop.currencies %}
                <li><a href="{{ currency.url | url }}" rel="nofollow" title="{{ currency.title }}">{{ currency.symbol }}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
            {% endif %}
          </div>
          <ul class="header-links pull-right paddinglg">
            {% if page.account %}
            <li><a href="{{ 'account/logout' | url }}" title="{{ 'Logout' | t }}">{{ 'Logout' | t }}</a></li>
            <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}">{{ 'My account' | t }}</a></li>
            {% else %}
            <li><a href="{{ 'account' | url }}" title="{{ 'My account' | t }}"><span class="glyphicon glyphicon-log-in"></span> {{ 'Login' | t }}</a></li>
            <li><a href="{{ 'account/register' | url }}" title="{{ 'Register' | t }}"><span class="glyphicon glyphicon-user"></span><span class="glyphicon glyphicon-plus"></span> {{ 'Register' | t }}</a></li>
            {% endif %}
          </ul>
        </div>       
      </div><!-- row -->
      {% endif %}
      
      {% if theme.weergave_menu == '2laags' %}
      <nav class="menu2laags navbar navbar-fixed-top yamm {% if theme.weergave_usp_balk == 'sticky' %}stickyusps{% endif %}" role="navigation" id="stickynav">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="searchmobile">
          <form class="navbar-form" role="search" action="{{ 'search' | url }}" method="get" id="form_search3">
            <div class="input-group">
              <input type="text" class="form-control" autocomplete="off" placeholder="{{ 'Search' | t }}" name="q" id="srch-3">
              <div class="input-group-btn">
                <button class="btn btn-search" type="submit" title="{{ 'Search' | t }}" onclick="$('#form_search3').submit();"><span class="glyphicon glyphicon-search"></span>
                </button>
              </div>
            </div>
          </form>
        </div>     
     
        {% if theme.weergave_menu == '2laags' %}
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="{% if template == 'pages/index.rain' %}active{% endif %}"><a href="{{ '' | url }}" title="{{ 'Home' | t }}">{{ 'Home' | t }}</a></li>
            {% for category in shop.categories %}
            <li class="{% if category.url is active %}active{% endif %}{% if category.subs %} dropdown yamm-fw{% endif %}">
              {% if category.subs %}<span class="togglesub dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">+</span>{% endif %}
              <a title="{{ category.title }}" href="{{ category.url | url }}"> {{ category.title }} {% if category.subs %}<span class="caret"></span>{% endif %}</a>
              {% if category.subs %}
              <ul class="dropdown-menu" role="menu">
                <li class="grid-demo"> 
                  <div class="row column">  
                    {% for category in category.subs %}
                    <div class="item">
                      <a title="{{ category.title }}" href="{{ category.url | url }}" class="subtitel">{{ category.title }}</a>
                      <ul class="list-unstyled" >
                        {% for category in category.subs %}
                        <li class="{% if category.url is active %} active{% endif %}">
                          <a class="" href="{{ category.url | url }}" title="{{ category.title }}">{{ category.title }}</a>
                        </li>
                        {% endfor %}  
                      </ul>
                    </div>
                    {% endfor %}
                  </div>
                 </li>  
               </ul>
              {% endif %}
            </li>
            {% endfor %}
             {% if not theme.hide_brands_menu %} {% if shop.brands %}
            <li><a href="{{ 'brands' | url }}" class="{% if category.url is active %}current{% endif %} {% if category.subs %}has-submenu{% else %}single-item{% endif %}" title="{{ 'Brands' | t }}">{{ 'Brands' | t }}</a></li>
            {% endif %} {% endif %}
            {% if not theme.hide_blog_menu %} {% for blog in shop.blogs %}
            <li class="{% if blog.url is active %}current{% endif %}"><a href="{{ blog.url | url }}">{{ blog.title }}</a></li>
            {% endfor %} {% endif %}
          </ul>
          <div id="cartContainer" class="cart-container hidden-xs hidden-sm">
            <a href="{{ 'cart' | url }}" title="{{ 'My shopping cart' | t }}" class="cart">
              <span class="glyphicon glyphicon-shopping-cart"></span> <span class="cart-total-quantity">({{ page.cart.products | length }})</span>
            </a>
            <div class="view-cart col-md-3">
              {% include 'snippets/products-cart.rain' %}
              <div class="total">
                {{ 'Grand total' | t }}:<span class="pull-right"><strong>{{ page.cart.total.price | money }}</strong></span>
                <div class="price-tax">{% if shop.b2b %}{{ 'Excl. VAT' | t }}{% else %}{{ 'Incl. VAT' | t }}{% endif %} {% if shop.settings.legal.mode == 'strict' %} {{ 'Excl. $1' | t('<a href="' ~ 'service/shipping-returns' | url ~ '" target="_blank">' ~ 'Shipping costs' | t ~ '</a>') | raw }} {% endif %}
                </div>
              </div>
              <a href="{{ 'cart' | url }}" class="btn btn-sm btn-view">{{ 'Checkout' | t }}</a>
            </div>
          </div>
          <div class="stickysearch">
            <span class="glyphicon glyphicon-search"></span>
          </div><!-- stickysearch -->
          <div class="view-search hidden">
            <form class="navbar-form" role="search" action="{{ 'search' | url }}" method="get" id="form_search4">
              <div class="input-group">
